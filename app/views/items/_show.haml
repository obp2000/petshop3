%fieldset.ui-widget-content
  %h3= @object.name
  - [ "size", "colour" ].each do |attr|
    = render :partial => "shared/#{attr}", :collection => @object.send( attr.tableize ) rescue nil 
  = @object.category.name rescue nil
  = @object.type.constantize.season_name
  %p
    = roubles @object.price
    = RUB  
  %p= @object.blurb
  .thumbs.yoxview= render :partial => "shared/photo", :collection => @object.photos
